		{
			"listen": "prerequest",
			"script": {
				"id": "f7199f25-d530-42ce-abda-8c9a0b49e8ed",
				"type": "text/javascript",
				"packages": {},
				"exec": [
					"// --- Set HTTP method dynamically from iteration data ---\r",
					"let method = pm.iterationData.get(\"method\") || pm.iterationData.get(\"METHOD\");\r",
					"if (method) {\r",
					"    pm.request.method = method.toUpperCase();\r",
					"}\r",
					"\r",
					"// --- Get raw endpoint from iteration data, fallback to \"/\" ---\r",
					"let rawEndpoint = pm.iterationData.get(\"endpoint\") || \"/\";\r",
					"\r",
					"// --- Function to replace placeholders like {{fileId}} or :patientId ---\r",
					"function replacePlaceholder(str, placeholder, value) {\r",
					"    if (!value) return str;\r",
					"    return str.replace(new RegExp(`{{${placeholder}}}`, 'g'), value)\r",
					"              .replace(new RegExp(`:${placeholder}`, 'g'), value);\r",
					"}\r",
					"\r",
					"// --- List of placeholders you want to substitute ---\r",
					"const placeholders = [\"patientId\", \"fileId\", \"dieticianId\", \"userId\"];\r",
					"\r",
					"// --- Replace placeholders in the rawEndpoint using environment or variables ---\r",
					"placeholders.forEach(ph => {\r",
					"    const val = pm.environment.get(ph) || pm.variables.get(ph);\r",
					"    rawEndpoint = replacePlaceholder(rawEndpoint, ph, val);\r",
					"});\r",
					"\r",
					"// --- Substitute any remaining environment/collection variables inside the string ---\r",
					"let substitutedEndpoint = pm.variables.replaceIn(rawEndpoint).trim();\r",
					"\r",
					"// --- Clean leading slashes from the endpoint to normalize ---\r",
					"substitutedEndpoint = substitutedEndpoint.replace(/^\\/+/, \"\");\r",
					"\r",
					"// --- Save final endpoint to a variable for request URL usage ---\r",
					"pm.variables.set(\"dynamicEndpoint\", \"/\" + substitutedEndpoint);\r",
					"\r",
					"// --- Authorization header handling ---\r",
					"const authType = pm.iterationData.get(\"authType\") || \"No Auth\";\r",
					"\r",
					"const tokenMap = {\r",
					"    \"Dietician Token\": \"dieticianToken\",\r",
					"    \"Admin Token\": \"adminToken\",\r",
					"    \"Expired Token\": \"expiredToken\"\r",
					"};\r",
					"\r",
					"if (authType in tokenMap) {\r",
					"    const token = pm.environment.get(tokenMap[authType]);\r",
					"    if (token) {\r",
					"        pm.request.headers.upsert({\r",
					"            key: \"Authorization\",\r",
					"            value: \"Bearer \" + token\r",
					"        });\r",
					"    } else {\r",
					"        // Remove header if token is missing\r",
					"        pm.request.headers.remove(\"Authorization\");\r",
					"    }\r",
					"} else {\r",
					"    // Remove Authorization header if no auth is needed or unrecognized type\r",
					"    pm.request.headers.remove(\"Authorization\");\r",
					"}\r",
					"\r",
					"// --- Debug logging ---\r",
					"console.log(\"🧪 Scenario:\", pm.iterationData.get(\"scenario\"));\r",
					"console.log(\"📮 Method:\", pm.request.method);\r",
					"console.log(\"🔐 Auth Type:\", authType);\r",
					"console.log(\"🔗 Endpoint (raw):\", pm.iterationData.get(\"endpoint\"));\r",
					"console.log(\"📁 Endpoint (substituted):\", substitutedEndpoint);\r",
					"console.log(\"🌐 Full URL:\", (pm.environment.get(\"base_url\") || \"\") + \"/\" + substitutedEndpoint);\r",
					""
				]
			}
