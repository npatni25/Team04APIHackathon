				{
					"name": "Get Patient Morbidity Details",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"id": "694d7ddb-ec72-4242-998a-6fa02da83266",
								"exec": [
									"\r",
									"// ✅ Get current scenario from test data\r",
									"let scenario = pm.iterationData.get(\"scenario\");\r",
									"\r",
									"// ✅ Get auth type (Dietician, Admin, etc.)\r",
									"let authType = pm.iterationData.get(\"authType\");\r",
									"\r",
									"// ✅ Set Authorization header based on authType\r",
									"if (authType === \"Dietician Token\") {\r",
									"    pm.request.headers.upsert({\r",
									"        key: \"Authorization\",\r",
									"        value: \"Bearer \" + pm.environment.get(\"dieticianToken\")\r",
									"    });\r",
									"} else if (authType === \"Admin Token\") {\r",
									"    pm.request.headers.upsert({\r",
									"        key: \"Authorization\",\r",
									"        value: \"Bearer \" + pm.environment.get(\"adminToken\")\r",
									"    });\r",
									"} else if (authType === \"Expired Token\") {\r",
									"    pm.request.headers.upsert({\r",
									"        key: \"Authorization\",\r",
									"        value: \"Bearer \" + pm.environment.get(\"expiredToken\")\r",
									"    });\r",
									"} else if (authType === \"No Auth\") {\r",
									"    if (pm.request.headers.has(\"Authorization\")) {\r",
									"        pm.request.headers.remove(\"Authorization\");\r",
									"    }\r",
									"}\r",
									"\r",
									"// ✅ Get dynamic endpoint from JSON data\r",
									"let endpoint = pm.iterationData.get(\"endpoint\") || \"/\";\r",
									"\r",
									"// Trim whitespace\r",
									"endpoint = endpoint.trim();\r",
									"\r",
									"// Replace placeholders like {{patientId}} or :patientId with environment variables if present\r",
									"\r",
									"// Helper function to replace placeholders\r",
									"function replacePlaceholder(str, placeholder, value) {\r",
									"    return str.replace(new RegExp(`{{${placeholder}}}`, 'g'), value)\r",
									"              .replace(new RegExp(`:${placeholder}`, 'g'), value);\r",
									"}\r",
									"\r",
									"// List of possible placeholders to replace\r",
									"const placeholders = [\"patientId\", \"dieticianId\", \"fileId\", \"userId\"];\r",
									"\r",
									"// Replace all placeholders found in endpoint\r",
									"placeholders.forEach(ph => {\r",
									"    let val = pm.environment.get(ph) || pm.variables.get(ph);\r",
									"    if (val) {\r",
									"        endpoint = replacePlaceholder(endpoint, ph, val);\r",
									"    }\r",
									"});\r",
									"\r",
									"// If after replacement endpoint is empty or just whitespace, fallback to \"/\"\r",
									"if (!endpoint || endpoint.trim() === \"\") {\r",
									"    endpoint = \"/\";\r",
									"}\r",
									"\r",
									"// ✅ Set it as a local variable to use in the request URL\r",
									"pm.variables.set(\"dynamicEndpoint\", endpoint);\r",
									"\r",
									"// ✅ Debug logs\r",
									"console.log(\"🧪 Scenario:\", scenario);\r",
									"console.log(\"🔐 Auth Type:\", authType);\r",
									"console.log(\"🔗 Endpoint after replacement:\", endpoint);\r",
									"\r",
									"// Also log full URL (assuming base_url set in environment)\r",
									"let baseUrl = pm.environment.get(\"base_url\") || \"\";\r",
									"// Remove trailing slash from baseUrl if present to avoid double slash\r",
									"if (baseUrl.endsWith(\"/\")) {\r",
									"    baseUrl = baseUrl.slice(0, -1);\r",
									"}\r",
									"// Remove leading slash from endpoint if present to avoid double slash after trimming baseUrl\r",
									"let cleanEndpoint = endpoint.startsWith(\"/\") ? endpoint : \"/\" + endpoint;\r",
									"\r",
									"console.log(\"🌐 Full Request URL:\", baseUrl + cleanEndpoint);"
								],
								"type": "text/javascript",
								"packages": {}
